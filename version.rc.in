#include <winver.h>

VS_VERSION_INFO VERSIONINFO
 FILEVERSION     @MYTHOS_VERSION_COMMA@
 PRODUCTVERSION  @MYTHOS_VERSION_COMMA@
 FILEFLAGSMASK   0x3fL
 FILEFLAGS       0x0L
 FILEOS          0x4L
 FILETYPE        0x1L
 FILESUBTYPE     0x0L
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName",      "Mythos Engine"
      VALUE "FileDescription",  "Mythos Engine Editor"
      VALUE "FileVersion",      "@MYTHOS_VERSION@"
      VALUE "ProductName",      "Mythos"
      VALUE "ProductVersion",   "@MYTHOS_VERSION@"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1200
  END
END